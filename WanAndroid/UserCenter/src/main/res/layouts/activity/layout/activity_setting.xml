<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              style="@style/MatchMatch.Vertical">

    <zs.xmx.baselibrary.weight.HeadBar
        android:id="@+id/mHeadBar"
        style="@style/MatchWrap"
        android:background="@color/common_blue"
        app:titleText="设置"/>

    <TextView
        android:id="@+id/mUserProtocolTv"
        style="@style/SettingItemStyle"
        android:text="用户协议"/>

    <TextView
        android:id="@+id/mFeedBackTv"
        style="@style/SettingItemStyle"
        android:text="反馈意见"/>

    <TextView
        android:id="@+id/mAboutTv"
        style="@style/SettingItemStyle"
        android:text="关于"/>

    <Button
        android:id="@+id/mLogoutBtn"
        style="@style/FullBtnStyle"
        android:enabled="true"
        android:text="退出登录"
        />
</LinearLayout>
